//program to find whether a user is eligible to vote or not
#include <iostream>
#include <typeinfo>

using namespace std;

int main()
{
	int age;

	cout<<"Enter your age: ";
	cin>>age;
	
	
	if (age>=18)
	{
		cout<<"\n Yes, you are eligible for voting:)\n";
	}	
	else if(age<18)
	{
		cout<<"\n Sorry!! You are not eligible to vote\n";
	}
	
	return 0;
}
